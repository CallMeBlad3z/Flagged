{
  "expo": {
    "name": "Flagged",
    "slug": "flagged",
    "version": "1.0.0",
    "sdkVersion": "51.0.0",
    "icon": "./assets/images/app_icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "scheme": "flagged",
    "orientation": "portrait",
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsVersion": "11.8.0"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/BonaNova-Regular.ttf",
            "./assets/fonts/BonaNova-Bold.ttf",
            "./assets/fonts/BonaNova-Italic.ttf",
            "./assets/fonts/SourceSans3-Black.ttf",
            "./assets/fonts/SourceSans3-BlackItalic.ttf",
            "./assets/fonts/SourceSans3-Bold.ttf",
            "./assets/fonts/SourceSans3-BoldItalic.ttf",
            "./assets/fonts/SourceSans3-ExtraBold.ttf",
            "./assets/fonts/SourceSans3-ExtraBoldItalic.ttf",
            "./assets/fonts/SourceSans3-ExtraLight.ttf",
            "./assets/fonts/SourceSans3-ExtraLightItalic.ttf",
            "./assets/fonts/SourceSans3-Italic.ttf",
            "./assets/fonts/SourceSans3-Light.ttf",
            "./assets/fonts/SourceSans3-LightItalic.ttf",
            "./assets/fonts/SourceSans3-Medium.ttf",
            "./assets/fonts/SourceSans3-MediumItalic.ttf",
            "./assets/fonts/SourceSans3-Regular.ttf",
            "./assets/fonts/SourceSans3-SemiBold.ttf",
            "./assets/fonts/SourceSans3-SemiBoldItalic.ttf"
          ]
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/bg_onboarding.png",
            "./assets/images/logo_black.png",
            "./assets/images/logo_onboarding.png",
            "./assets/images/logo_white.png"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "expo-secure-store"
    ],
    "android": {
      "package": "com.blad3z.flagged",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png"
      }
    },
    "extra": {
      "eas": {
        "projectId": "85985c89-5fff-4108-9e14-6abcde85e42c"
      }
    }
  }
}
